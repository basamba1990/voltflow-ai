{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "pnpm build",
  "engines": {
    "node": ">=22.12.1"
  }
}
